# Write your unique_values function here:
def unique_values(my_dictionary):
  list = []
  for value in my_dictionary.values():
    if value not in list:
      list.append(value)
  return len(list)
# Test
print(unique_values({0:3, 1:1, 4:1, 5:3}))
# should print 2
print(unique_values({0:3, 1:3, 4:3, 5:3}))
# should print 1
