# Write your sum_values function here:
def sum_values(my_dictionary):
  dictionary = 0
  for word in my_dictionary.values():
    dictionary += word
  return dictionary
# Test:
print(sum_values({"milk":5, "eggs":2, "flour": 3}))
# should print 10
print(sum_values({10:1, 100:2, 1000:3}))
# should print 6
